# MikroClaw Installer - Environment Variables
# Copy this file to .env and fill in your secrets
# NEVER commit .env to version control!

# RouterOS Connection
MIKROCLAW_ROUTEROS_HOST=192.168.100.1
MIKROCLAW_ROUTEROS_PORT=7778
MIKROCLAW_ROUTEROS_USERNAME=jefferson
MIKROCLAW_ROUTEROS_PASSWORD=your-strong-password-here

# AI Provider API Keys (use only the one for your provider)
MIKROCLAW_CEREBRAS_API_KEY=csk-your-key-here
MIKROCLAW_OPENROUTER_API_KEY=sk-or-v1-your-key-here
MIKROCLAW_OPENAI_API_KEY=sk-your-key-here
MIKROCLAW_ZAI_API_KEY=zai-your-key-here

# MemU Cloud Memory
MIKROCLAW_MEMU_KEY=mu-your-memu-key-here

# Bot Integrations (optional)
MIKROCLAW_TELEGRAM_TOKEN=your-telegram-bot-token
MIKROCLAW_DISCORD_WEBHOOK=https://discord.com/api/webhooks/...

# Container Registry (optional)
MIKROCLAW_REGISTRY_USERNAME=your-username
MIKROCLAW_REGISTRY_TOKEN=ghp-your-token-here
